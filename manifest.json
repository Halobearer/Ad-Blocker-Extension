{
  "manifest_version": 3,
  "name": "Prime Ad Blocker",
  "description": "Blocks Ads, I think ??!",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "icons": {
    "16": "icons/logo_16x16.png",
    "48": "icons/logo_48x48.png",
    "128": "icons/logo_128x128.png"
  }
}